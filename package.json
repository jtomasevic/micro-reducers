  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jtomasevic/micro-reducers/issues"
  },
  "homepage": "https://github.com/jtomasevic/micro-reducers#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.0",
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-flow-strip-types": "^7.8.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "flow-bin": "^0.115.0",
    "flow-copy-source": "^2.0.9"
  },
  "dependencies": {
    "core-js": "^3.1.4",
    "react": "^16.8.6"
